{
  "name": "find-my-el",
  "version": "2.0.1",
  "description": "Locates the DOM element closest to a given set of coordinates",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.min.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bili",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "example": "poi --serve",
    "example:build": "rm -rf example/dist && poi --prod",
    "example:deploy": "npm run example:build && gh-pages -d example/dist",
    "version": "npm run example:deploy"
  },
  "keywords": [
    "DOM",
    "closest",
    "find",
    "coordinates"
  ],
  "author": "Collin Henderson <collin@syropia.net>",
  "license": "MIT",
  "repository": "https://github.com/syropian/find-my-el",
  "bili": {
    "input": "src/index.ts",
    "output": {
      "format": [
        "cjs",
        "umd-min",
        "es"
      ],
      "moduleName": "FindMyEl"
    }
  },
  "poi": {
    "entry": "example/index.js",
    "output": {
      "dir": "example/dist",
      "publicUrl": "/find-my-el/"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "bili": "^4.8.1",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "poi": "^12.7.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "vue": "^2.6.10",
    "vue-draggable-resizable": "^1.7.1",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {}
}
